    "staticGraphs": {
      "ecommerceOrderService": {
        "description": "E-commerce order processing flow with inventory, payment, and notifications",
        "request": {
          "graphId": "ecommerce-order-service",
          "version": "1.0.0",
          "nodes": [
            {
              "nodeId": "endpoint-create-order",
              "type": "ENDPOINT",
              "name": "POST /api/orders",
              "label": "Create Order API",
              "attributes": {
                "httpMethod": "POST",
                "path": "/api/orders",
                "produces": "application/json",
                "consumes": "application/json"
              }
            },
            {
              "nodeId": "endpoint-get-order",
              "type": "ENDPOINT",
              "name": "GET /api/orders/{id}",
              "label": "Get Order API",
              "attributes": {
                "httpMethod": "GET",
                "path": "/api/orders/{id}",
                "produces": "application/json"
              }
            },
            {
              "nodeId": "service-order-process",
              "type": "METHOD",
              "name": "OrderService.processOrder",
              "label": "Process Order",
              "attributes": {
                "class": "com.ecommerce.order.OrderService",
                "visibility": "PUBLIC",
                "returnType": "Order"
              }
            },
            {
              "nodeId": "service-order-validate",
              "type": "METHOD",
              "name": "OrderService.validateOrder",
              "label": "Validate Order",
              "attributes": {
                "class": "com.ecommerce.order.OrderService",
                "visibility": "PRIVATE",
                "returnType": "ValidationResult"
              }
            },
            {
              "nodeId": "service-inventory-check",
              "type": "METHOD",
              "name": "InventoryService.checkAvailability",
              "label": "Check Inventory",
              "attributes": {
                "class": "com.ecommerce.inventory.InventoryService",
                "visibility": "PUBLIC",
                "returnType": "AvailabilityResult"
              }
            },
            {
              "nodeId": "service-inventory-reserve",
              "type": "METHOD",
              "name": "InventoryService.reserveItems",
              "label": "Reserve Items",
              "attributes": {
                "class": "com.ecommerce.inventory.InventoryService",
                "visibility": "PUBLIC",
                "returnType": "ReservationResult"
              }
            },
            {
              "nodeId": "service-payment-process",
              "type": "METHOD",
              "name": "PaymentService.processPayment",
              "label": "Process Payment",
              "attributes": {
                "class": "com.ecommerce.payment.PaymentService",
                "visibility": "PUBLIC",
                "returnType": "PaymentResult"
              }
            },
            {
              "nodeId": "service-notification-send",
              "type": "METHOD",
              "name": "NotificationService.sendOrderConfirmation",
              "label": "Send Confirmation",
              "attributes": {
                "class": "com.ecommerce.notification.NotificationService",
                "visibility": "PUBLIC",
                "async": true
              }
            },
            {
              "nodeId": "topic-order-events",
              "type": "TOPIC",
              "name": "order-events",
              "label": "Order Events Topic",
              "attributes": {
                "broker": "kafka",
                "partitions": 6,
                "replicationFactor": 3
              }
            },
            {
              "nodeId": "topic-inventory-updates",
              "type": "TOPIC",
              "name": "inventory-updates",
              "label": "Inventory Updates Topic",
              "attributes": {
                "broker": "kafka",
                "partitions": 3
              }
            },
            {
              "nodeId": "external-payment-gateway",
              "type": "EXTERNAL",
              "name": "Stripe API",
              "label": "Payment Gateway",
              "attributes": {
                "provider": "stripe",
                "endpoint": "https://api.stripe.com/v1",
                "timeout": 30000
              }
            },
            {
              "nodeId": "external-email-service",
              "type": "EXTERNAL",
              "name": "SendGrid",
              "label": "Email Service",
              "attributes": {
                "provider": "sendgrid",
                "endpoint": "https://api.sendgrid.com/v3"
              }
            }
          ],
          "edges": [
            {
              "edgeId": "e1",
              "sourceNodeId": "endpoint-create-order",
              "targetNodeId": "service-order-process",
              "type": "CALL",
              "label": "invokes"
            },
            {
              "edgeId": "e2",
              "sourceNodeId": "service-order-process",
              "targetNodeId": "service-order-validate",
              "type": "CALL",
              "label": "validates"
            },
            {
              "edgeId": "e3",
              "sourceNodeId": "service-order-process",
              "targetNodeId": "service-inventory-check",
              "type": "CALL",
              "label": "checks availability"
            },
            {
              "edgeId": "e4",
              "sourceNodeId": "service-order-process",
              "targetNodeId": "service-inventory-reserve",
              "type": "CALL",
              "label": "reserves items"
            },
            {
              "edgeId": "e5",
              "sourceNodeId": "service-order-process",
              "targetNodeId": "service-payment-process",
              "type": "CALL",
              "label": "processes payment"
            },
            {
              "edgeId": "e6",
              "sourceNodeId": "service-payment-process",
              "targetNodeId": "external-payment-gateway",
              "type": "HTTP",
              "label": "charges card"
            },
            {
              "edgeId": "e7",
              "sourceNodeId": "service-order-process",
              "targetNodeId": "topic-order-events",
              "type": "PRODUCES",
              "label": "publishes event"
            },
            {
              "edgeId": "e8",
              "sourceNodeId": "service-inventory-reserve",
              "targetNodeId": "topic-inventory-updates",
              "type": "PRODUCES",
              "label": "publishes update"
            },
            {
              "edgeId": "e9",
              "sourceNodeId": "topic-order-events",
              "targetNodeId": "service-notification-send",
              "type": "CONSUMES",
              "label": "triggers notification"
            },
            {
              "edgeId": "e10",
              "sourceNodeId": "service-notification-send",
              "targetNodeId": "external-email-service",
              "type": "HTTP",
              "label": "sends email"
            }
          ],
          "metadata": {
            "application": "order-service",
            "team": "commerce-platform",
            "environment": "production",
            "buildNumber": "1234",
            "gitCommit": "abc123def456"
          }
        }
      },
