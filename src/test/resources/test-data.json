          "edges": [
            {"edgeId": "e1", "sourceNodeId": "endpoint-login", "targetNodeId": "service-auth-authenticate", "type": "CALL"},
            {"edgeId": "e2", "sourceNodeId": "service-auth-authenticate", "targetNodeId": "service-user-find", "type": "CALL"},
            {"edgeId": "e3", "sourceNodeId": "service-auth-authenticate", "targetNodeId": "service-auth-validate-credentials", "type": "CALL"},
            {"edgeId": "e4", "sourceNodeId": "service-auth-authenticate", "targetNodeId": "service-token-generate", "type": "CALL"},
            {"edgeId": "e5", "sourceNodeId": "service-auth-authenticate", "targetNodeId": "service-session-create", "type": "CALL"},
            {"edgeId": "e6", "sourceNodeId": "service-session-create", "targetNodeId": "external-redis-cache", "type": "CACHE_PUT"},
            {"edgeId": "e7", "sourceNodeId": "endpoint-refresh", "targetNodeId": "service-token-validate", "type": "CALL"},
            {"edgeId": "e8", "sourceNodeId": "service-token-validate", "targetNodeId": "external-redis-cache", "type": "CACHE_GET"}
          ],
          "metadata": {
            "application": "auth-service",
            "team": "identity",
            "environment": "production"
          }
        }
      },
      "notificationService": {
        "description": "Multi-channel notification service",
        "request": {
          "graphId": "notification-service",
          "version": "1.5.0",
          "nodes": [
            {"nodeId": "topic-notifications", "type": "TOPIC", "name": "notification-requests", "label": "Notification Queue"},
            {"nodeId": "consumer-notification", "type": "METHOD", "name": "NotificationConsumer.handleNotification", "label": "Handle Notification"},
            {"nodeId": "service-router", "type": "METHOD", "name": "NotificationRouter.route", "label": "Route Notification"},
            {"nodeId": "service-email", "type": "METHOD", "name": "EmailNotificationHandler.send", "label": "Send Email"},
            {"nodeId": "service-sms", "type": "METHOD", "name": "SmsNotificationHandler.send", "label": "Send SMS"},
            {"nodeId": "service-push", "type": "METHOD", "name": "PushNotificationHandler.send", "label": "Send Push"},
            {"nodeId": "external-sendgrid", "type": "EXTERNAL", "name": "SendGrid", "label": "Email Provider"},
            {"nodeId": "external-twilio", "type": "EXTERNAL", "name": "Twilio", "label": "SMS Provider"},
            {"nodeId": "external-firebase", "type": "EXTERNAL", "name": "Firebase", "label": "Push Provider"}
          ],
          "edges": [
            {"edgeId": "e1", "sourceNodeId": "topic-notifications", "targetNodeId": "consumer-notification", "type": "CONSUMES"},
            {"edgeId": "e2", "sourceNodeId": "consumer-notification", "targetNodeId": "service-router", "type": "CALL"},
            {"edgeId": "e3", "sourceNodeId": "service-router", "targetNodeId": "service-email", "type": "CALL"},
            {"edgeId": "e4", "sourceNodeId": "service-router", "targetNodeId": "service-sms", "type": "CALL"},
            {"edgeId": "e5", "sourceNodeId": "service-router", "targetNodeId": "service-push", "type": "CALL"},
            {"edgeId": "e6", "sourceNodeId": "service-email", "targetNodeId": "external-sendgrid", "type": "HTTP"},
            {"edgeId": "e7", "sourceNodeId": "service-sms", "targetNodeId": "external-twilio", "type": "HTTP"},
            {"edgeId": "e8", "sourceNodeId": "service-push", "targetNodeId": "external-firebase", "type": "HTTP"}
          ],
          "metadata": {
            "application": "notification-service",
            "team": "platform"
          }
        }
      }
